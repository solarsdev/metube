{
  "name": "metube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanilla and NodeJS",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec babel-node src/init.js --delay 3.5",
    "start": "node build/init.js",
    "build:server": "babel src --out-dir build --ignore '.scss' --ignore 'static'",
    "copyAll": "mkdir -p build/logs && touch build/logs/access.log && cp -R src/uploads build && cp -R src/views build",
    "build": "npm run build:server && npm run copyAll",
    "prebuild": "rm -rf build"
  },
  "author": "solarsdev",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "moment-timezone": "^0.5.33",
    "mongoose": "^5.12.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "core-js": "^3.9.1",
    "nodemon": "^2.0.7"
  }
}
